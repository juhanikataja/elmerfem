Check Keywords "Warn"

Header :: Mesh DB "." "square"

Simulation
  Max Output Level = 7
  Coordinate System = Cartesian
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
  Post File = "case.vtu"
End

Body 1
  Equation = 1
  Material = 1
  Body Force = 1
  Initial Condition = 1
End

Initial Condition 1
  Field = Real 1.0
End 

Material 1
  Name = "Ideal" 
!  diffusion coefficient = Variable "field"
!    Real MATC "sqrt(tx)"
  diffusion coefficient = Variable "mysqrt(field)"
    Real MATC "tx"
End

Body Force 1
  Field Source = Real 10.0
End 

Equation 1
  Active Solvers(1) = 1
End 

Solver 1
  Equation = "ModelFun"
  Variable = "Field"
  Procedure = "ModelFun" "AdvDiffSolver"

  Nonlinear System Max Iterations = 10
  Nonlinear System Convergence Tolerance = 1.0e-5
  Linear System Solver = Direct
  Steady State Convergence Tolerance = 1e-9
End

Boundary Condition 1
  Target Boundaries(1) = 1
  Field = 1.0
End


Solver 1 :: Reference Norm = Real 1.2786092
Solver 1 :: Reference Norm Tolerance = Real 1.0E-5
