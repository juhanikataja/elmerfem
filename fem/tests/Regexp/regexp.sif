Header
  CHECK KEYWORDS Warn
  Mesh DB "." "square"
End

Simulation
  Max Output Level = 5
  Coordinate System = Cartesian
  Coordinate Mapping(3) = 1 2 3
  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 1
! Post File = "DGsolution.ep"
End

Constants
End

Body 1
  Equation = 1
  Material = 1
  Body force = 1
End

Body 2
  Equation = 1
  Material = 1
  Body force = 1
End

Equation 1
  active solvers(1) = 1
End

Material 1
  Density = 1
End

Solver 1
  Equation = "regexp solver"
  Procedure = File "./regexp" "RegexpTester"
  Variable = "Potential"
  Variable Dofs = 1

  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations  = 1000
  Linear System Preconditioning = ILUT
  Linear System ILUT Tolerance  = 0.005
  Linear System Convergence Tolerance = 1.0e-6
  Linear System Residual Output = 20
  Optimize Bandwidth = True
  str to parse = file "try parsing this | here's something -- and this in different place (3)"
  ! str to parse = string "Hello, world (3)" 
End


Solver 2
  exec solver = never
  Equation = "result output"
  Procedure = "ResultOutputSolve" "ResultOutputSolver"
  Output File Name = case_e
  Save Geometry IDs = Logical False
  Vtu Format = Logical True

!  Save Bulk Only = Logical True
!  Discontinuous Bodies = Logical True
  Discontinuous Galerkin = Logical True
End 

Body Force 1
  Source = Real 1.0
End

Boundary Condition 1
  Target Boundaries(2) = 1 2 
End

Solver 1 :: Reference Norm = 0.0
